#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/run-script-os@1.1.6/node_modules/run-script-os/index.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/run-script-os@1.1.6/node_modules/run-script-os/index.js" "$@"
fi
